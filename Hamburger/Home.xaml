<Page
    x:Class="Hamburger.Home"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Hamburger"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <RelativePanel Margin="20,5,0,0">
            <Image Source="Assets/Business1.png" Name="FnImg" RelativePanel.AlignLeftWithPanel="True" MaxHeight="200" MinWidth="400" Margin="30,10,0,0"/>
            <Image Source="Assets/Cake.png" RelativePanel.RightOf="FnImg" MaxHeight="200" MinWidth="400" Margin="30,10,0,-0.4" />
            <Image Source="Assets/Landscape.png" RelativePanel.Below="FnImg" MaxHeight="480" MinWidth="990" Margin="30,10,-585.4,-0.4"/>
        </RelativePanel>
    </Grid>
</Page>
